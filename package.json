{
  "name": "agent-sandbox",
  "version": "1.0.0",
  "description": "Lightweight Agent Sandbox - Run AI tasks in isolated Docker containers",
  "main": "api/server.js",
  "scripts": {
    "start": "node api/server.js",
    "dev": "node --watch api/server.js",
    "test": "node --test tests/*.test.js",
    "build:image": "docker build -t agent-executor:latest ./docker",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "logs": "docker-compose logs -f"
  },
  "keywords": ["agent", "sandbox", "docker", "ai"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.11.7",
    "ioredis": "^5.4.1",
    "dockerode": "^4.0.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
