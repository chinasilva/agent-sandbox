{
  "version": 2,
  "buildCommand": "npm install --no-optional",
  "installCommand": "npm install --no-optional",
  "framework": null,
  "functions": {
    "api/server.js": {
      "runtime": "nodejs22.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/health",
      "dest": "/api/server.js"
    }
  ]
}
