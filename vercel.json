{
  "version": 2,
  "framework": null,
  "buildCommand": "npm install --no-optional",
  "installCommand": "npm install --no-optional",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/www/index.html"
    }
  ]
}
